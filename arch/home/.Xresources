! Απο "Gentoo wiki". Για τα fonts.
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
Xft.autohint:   false
Xft.lcdfilter:  lcddefault

! Scrollbar disable:
URxvt.scrollBar: false

! fonts:
URxvt.font: xft:Inconsolata:size=10
URxvt.letterSpace: -1

! special
URxvt.foreground:   #c5c8c6
URxvt.background:   #1d1f21
URxvt.cursorColor:  #c5c8c6

! black
URxvt.color0:       #282a2e
URxvt.color8:       #373b41

! red
URxvt.color1:       #a54242
URxvt.color9:       #cc6666

! green
URxvt.color2:       #8c9440
URxvt.color10:      #b5bd68

! yellow
URxvt.color3:       #de935f
URxvt.color11:      #f0c674

! blue
URxvt.color4:       #5f819d
URxvt.color12:      #81a2be

! magenta
URxvt.color5:       #85678f
URxvt.color13:      #b294bb

! cyan
URxvt.color6:       #5e8d87
URxvt.color14:      #8abeb7

! white
URxvt.color7:       #707880
URxvt.color15:      #c5c8c6

! Perls addons:
URxvt.perl-ext-common: default,matcher,keyboard-select,url-select,selection-to-clipboard

! Clickable links: 
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5

! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate

! links-select:
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

! selection-to-clipboard
! copy to system clipboard. Μπορώ να κάνω copy paste πχ στον browser. Μπορώ να κάνω paste στο urxvt με το control+shift+v εναλλακτικά του shift+insert.  

